﻿@model  Test.Models.StudentDto
@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you want to delete this student?</h2>
<a href="/Student/List">Cancel</a>
<h3>Name: @Model.first_name @Model.last_name</h3>
<form action="/Student/Delete/@Model.student_id" method="POST">
    <input type="submit" value="Confirm Delete" />
</form>