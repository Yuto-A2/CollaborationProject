﻿@model IEnumerable<Test.Models.TeacherMealPlanDto>
@{
    ViewBag.Title = "Create";
}

<h2>Add Teacher Meal  Plan</h2>
<form method="POST" action="Create" name="TeacherMealPlanForm">


    <div>
        <label for="first_name">First Name:</label>
        <input type="text" name="first_name" id="first_name" />
    </div>

    <div>
        <label for="last_name">Lasts Name:</label>
        <input type="text" name="last_name" id="last_name" />
    </div>

    <div>
        <label for="plan_name">Plan Name:</label>
        <input type="text" name="plan_name" id="plan_name">
    </div>

    <div>
        <input type="submit" value="Add your meal plan" />
    </div>
</form>


